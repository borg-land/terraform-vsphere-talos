variable "vsphere_user" {
  description = "vSphere user name"
  type        = string
  default     = ""

  validation {
    condition     = var.vsphere_user != ""
    error_message = "Must provide vsphere user."
  }
}
variable "vsphere_password" {
  description = "vSphere password"
  sensitive   = true
  type        = string
  default     = ""

  validation {
    condition     = var.vsphere_password != ""
    error_message = "Must provide vsphere password."
  }
}
variable "vsphere_server" {
  description = "vCenter server FQDN or IP"
  type        = string
  default = ""

  validation {
    condition = var.vsphere_server != ""
    error_message = "Must provide vsphere server."
  }
}
variable "vsphere_unverified_ssl" {
  description = "Allow unverified SSL?"
  type = bool
  default = false
}

provider "vsphere" {
  vsphere_server = var.vsphere_server
  user           = var.vsphere_user
  password       = var.vsphere_password

  allow_unverified_ssl = var.vsphere_unverified_ssl
}

